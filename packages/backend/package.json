{
   "name": "modos",
   "version": "0.0.1",
   "description": "MoDoS REST API",
   "private": true,
   "scripts": {
      "start": "ts-node src/index.ts",
      "migration:create": "ts-node ./node_modules/.bin/typeorm migration:create --config ormconfig.ts",
      "migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate --config ormconfig.ts",
      "migrate": "ts-node ./node_modules/.bin/typeorm migration:run --config ormconfig.ts",
      "rollback": "ts-node ./node_modules/.bin/typeorm migration:revert --config ormconfig.ts",
      "synchronize": "ts-node src/config/synchronize.ts"
   },
   "devDependencies": {
      "@types/bcrypt": "^3.0.0",
      "@types/body-parser": "^1.19.0",
      "@types/cors": "^2.8.6",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.6",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/node": "^8.0.29",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "bcrypt": "^4.0.1",
      "body-parser": "^1.19.0",
      "class-validator": "^0.12.2",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "jsonwebtoken": "^8.5.1",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "swagger-ui-express": "^4.1.4",
      "typeorm": "0.2.24"
   },
   "engines": {
     "node": "^12.0.0"
   }
}
